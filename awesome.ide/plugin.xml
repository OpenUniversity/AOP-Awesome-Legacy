<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Sample Multi-page Editor"
            extensions="mpe"
            icon="icons/sample.gif"
            contributorClass="awesome.ide.editors.MultiPageEditorContributor"
            class="awesome.ide.editors.MultiPageEditor"
            id="awesome.ide.editors.MultiPageEditor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Awesome"
            id="awesome.ide">
      </category>
      <wizard
            name="Awesome Project"
            icon="icons/aw.jpg"
            category="awesome.ide"
            class="awesome.ide.wizards.NewAwesomeProjectWizard"
            id="awesome.ide.wizards.NewAwesomeProjectWizard">
      </wizard>
   </extension>

</plugin>
